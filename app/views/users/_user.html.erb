<li>
	<%= link_to image_tag("tantai.jpg", alt: "Avatar", size: '52x52'), user %>
	<%= link_to user.name, user %>
	<%= user.email %>
	<% if current_user.admin? && !current_user?(user) %>
		<%= link_to "Edit", edit_user_path(user) %>
		 |
		<%= link_to "Delete", user, method: :delete %>
	<% end %>
</li>


